[global]
rw=write
ioengine=libaio
iodepth=32
numjobs=1
bs=4m
;size=50G
direct=1
time_based=1
ramp_time=90
runtime=600
write_iops_log=/home/centos/Results/output.0
write_bw_log=/home/centos/Results/output.0
write_lat_log=/home/centos/Results/output.0

[job1]
filename=/dev/vdb


[job2]
filename=/dev/vdc
;write_iops_log=/home/centos/Results/output.1
;write_bw_log=/home/centos/Results/output.1
;write_lat_log=/home/centos/Results/output.1